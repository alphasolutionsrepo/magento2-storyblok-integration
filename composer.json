{
    "name": "medialounge/magento2-storyblok-integration-contentapi",
    "description": "Magento 2 extension to integrate Storyblok visual editor (using the Storyblok Content API)",
    "type": "magento2-module",
    "version": "2.2.0",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Javier Villanueva",
            "email": "javier@medialounge.co.uk"
        },
        {
            "name": "Roland Villemoes",
            "email": "rv@alpha-solutions.us"
        }
    ],
    "keywords": [
        "magento2",
        "magento2-module",
        "magento2-extension",
        "storyblok"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "MediaLounge\\Storyblok\\": ""
        }
    },
    "require": {
        "php": ">=8.3",
        "magento/framework": "^103.0",
        "magento/module-page-cache": "100.4.*",
        "magento/module-sitemap": "100.4.*",
        "storyblok/php-content-api-client": "^1.3.1",
        "storyblok/richtext-resolver": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0.0",
        "squizlabs/php_codesniffer": "^3.5",
        "magento/magento-coding-standard": "^6.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alphasolutionsrepo/magento2-storyblok-integration-contentapi.git"
        }
    ]
}
